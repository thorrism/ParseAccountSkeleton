<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="RaisedButton">
        <attr name="innerPadding" format="dimension" />
        <attr name="backgroundColor" format="color" />
        <attr name="contentColor" format="color" />
        <attr name="content" format="string" />
        <attr name="cornerRadius" format="dimension" />
        <attr name="raisedElevation" format="dimension" />
    </declare-styleable>

    <declare-styleable name="TextViewShadow">
        <attr name="textColor" format="color" />
        <attr name="text" format="string" />
        <attr name="textSize" format="dimension" />
        <attr name="textElevation" format="dimension" />
    </declare-styleable>

    <declare-styleable name="LabelView">
        <attr name="lineColor" format="color" />
        <attr name="lineThickness" format="dimension" />
        <attr name="linePadding" format="dimension" />
        <attr name="textPadding" format="dimension" />
    </declare-styleable>

    <declare-styleable name="MaterialLabelView">
        <attr name="labelColor" format="color" />
        <attr name="sidePadding" format="dimension" />
    </declare-styleable>

    <declare-styleable name="ShakeEditText">
        <attr name="defaultError" format="string"/>
    </declare-styleable>

</resources>